{
  "name": "@zzz-wiki-scrap/data",
  "version": "1.0.0",
  "description": "HoyoLab ZZZ Wikiからキャラクターデータを生成するプロジェクト",
  "main": "./data/index.ts",
  "types": "./data/index.ts",
  "exports": {
    "./characters": {
      "import": "./data/characters.ts",
      "require": "./data/characters.ts",
      "types": "./data/characters.ts"
    },
    "./factions": {
      "import": "./data/factions.ts",
      "require": "./data/factions.ts",
      "types": "./data/factions.ts"
    },
    "./bomps": {
      "import": "./data/bomps.ts",
      "require": "./data/bomps.ts",
      "types": "./data/bomps.ts"
    }
  },
  "files": [
    "data"
  ],
  "scripts": {
    "generate": "tsx src/main.ts",
    "generate:config": "ts-node --esm src/main-configurable.ts",
    "generate:bomps": "tsx src/main-bomp-generation.ts",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --run --coverage"
  },
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node",
    "compilerOptions": {
      "module": "ESNext",
      "moduleResolution": "node"
    }
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "ts-node": "^10.9.0",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}
